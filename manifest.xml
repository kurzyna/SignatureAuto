  <Id>cf06154b-60ae-4c9b-9a02-56a012d14b44</Id>
  <Version>1.0.0.0</Version>
  <ProviderName>Contoso</ProviderName>
  <DefaultLocale>en-US</DefaultLocale>
  <DisplayName DefaultValue="Stopka AKMF"/>
  <Description DefaultValue="Dodaj stopkÄ™"/>
<IconUrl DefaultValue="https://kurzyna.github.io/SignatureAuto/dist/assets/akmf_logo.png"/>
  <HighResolutionIconUrl DefaultValue="https://kurzyna.github.io/SignatureAuto/dist/assets/akmf_logo.png"/>
  <SupportUrl DefaultValue="https://www.contoso.com/help"/>
  <AppDomains>
